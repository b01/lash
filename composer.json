{
    "authors": [
        {
            "homepage": "http://www.kshabazz.net/",
            "name": "Khalifah Khalil Shabazz",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Whip\\Lash\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Whip\\Lash\\Test\\": "test"
        }
    },
    "description": "Whip web tools for validation.",
    "license": "MIT",
    "name": "kshabazz/whip-lash",
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpmd/phpmd": "^2.0",
        "psr/http-message": "^1.0",
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^2.0"
    },
    "scripts": {
        "units": "phpunit",
        "pcs": "phpcs -p --standard=PSR2 --severity=8 --colors src",
        "pmd": "phpmd src text cleancode --strict"
    }
}